<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tech2.demo.com.demo" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="tech2.demo.com.demo.ui.activities.Toptal"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="tech2.demo.com.demo.ui.activities.LoginActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="tech2.demo.com.demo.ui.activities.RegisterUserActivity"
            android:label="@string/title_activity_register_user"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="tech2.demo.com.demo.ui.activities.MealsActivity"
            android:label="@string/title_activity_meals"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="tech2.demo.com.demo.ui.activities.AdminActivity"
            android:label="@string/title_activity_admin"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>
